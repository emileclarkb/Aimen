using prague
include prague.core {engine}

using math4
instance math4.[Vector,Matrix,Quaternion]

using Sonata
using Rendering


obj prague {
    ref engine.Awake(engine.err status) {
        // error occurred on boot
        if status {
            engine.console.err(status);
        }
    }

    ref engine.Start() {
        engine.sampeScene("ParticleRender");
    }
}
